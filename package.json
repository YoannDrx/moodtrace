  "scripts": {
    "build": "next build --turbo",
    "type:generate": "next typegen .",
    "clean": "next lint --fix && tsc --noEmit && prettier --write .",
    "dev": "next dev --turbopack",
    "email": "email dev",
    "format": "prettier --write .",
    "postinstall": "prisma generate && pnpm type:generate",
    "knip": "knip-bun --config knip.json",
    "lint": "eslint . --fix",
    "start": "next start",
    "stripe-webhooks": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "ts": "tsc --noEmit",
    "vercel-build": "prisma generate && pnpm build && prisma migrate deploy",
    "test": "vitest",
    "test:e2e": "HEADLESS=0 playwright test --ui",
    "test:ci": "vitest run",
    "test:e2e:ci": "HEADLESS=TRUE playwright test",
    "lint:ci": "eslint .",
    "better-auth:migrate": "pnpm dlx @better-auth/cli generate --output prisma/schema/better-auth.prisma",
    "prisma:seed": "NODE_ENV=development prisma db seed",
    "prisma:migrate": "pnpm prisma migrate dev",
    "prisma:generate": "pnpm prisma generate"
  },
